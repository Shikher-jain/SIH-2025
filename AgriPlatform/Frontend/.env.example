# Environment Configuration
VITE_ENVIRONMENT=development

# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api
VITE_API_TIMEOUT=10000

# Map Configuration
VITE_MAP_API_KEY=your_maptiler_api_key_here
VITE_MAP_DEFAULT_CENTER_LAT=40.7128
VITE_MAP_DEFAULT_CENTER_LNG=-74.0060
VITE_MAP_DEFAULT_ZOOM=10

# Weather API Configuration
VITE_WEATHER_API_KEY=your_weather_api_key_here
VITE_WEATHER_API_URL=https://api.openweathermap.org/data/2.5

# Authentication Configuration
VITE_JWT_SECRET=your_jwt_secret_here
VITE_JWT_EXPIRES_IN=24h

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_MOCK_DATA=true

# Application Configuration
VITE_APP_NAME=AgriculturePlatform
VITE_APP_VERSION=1.0.0
VITE_SUPPORT_EMAIL=support@agriculture-platform.com